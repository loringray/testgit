.item-navigator{
  width: 200rpx;
  margin-right: 20rpx;
  display: inline-block;
}

/* .item-navigator:last-of-type{
  margin-right: 0;
} */
.item-navigator .item-group{
  width: 100%;
}

.item-group .thumbnail-group{
  width: 100%;
  height: 204rpx;
}

.thumbnail-group .thumbnail{
  width: 100%;
  height: 100%;
}
.item-group .item-title{
  font-size: 28rpx;
  text-align: center;
  margin-top: 20rpx;
  text-overflow: ellipsis;    /**溢出就显示... */
  overflow: hidden;
  margin-bottom: 20rpx;
}

